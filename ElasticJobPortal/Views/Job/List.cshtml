@model List<ElasticJobPortal.Models.Job>

<h2>Posted Jobs</h2>

<table class="table">
    <thead>
        <tr>
            <th>Title</th>
            <th>Company</th>
            <th>Job Type</th>
            <th>Description</th>
            <th>Skills</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var job in Model)
        {
            <tr>
                <td>@job.Title</td>
                <td>@job.Company</td>
                <td>@job.JobType</td>
                <td>@job.Description</td>
                <td>@job.SkillsCsv</td>
            </tr>
        }
    </tbody>
</table>
